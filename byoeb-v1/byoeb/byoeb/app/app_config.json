{
    "app":{
        "batch_size": 5,
        "azure_cognitive_endpoint": "https://cognitiveservices.azure.com/.default",
        "queue_provider": "azure_storage_queue",
        "db_provider": "azure_cosmos_mongo_db"
    },
    "channel": {
        "whatsapp": {
            "allowed_message_types": [
                "audio",
                "text"
            ],
            "reuse_client": true
        },
        "reaction": {
            "user": {
                "verified": "✅",
                "rejected": "❌",
                "pending": "⏳"
            },
            "expert": {
                "resolved": "✅",
                "pending": "⏳",
                "waiting": "✏️"
            }
        }
    },
    "databases": {
        "mongo_db":
        {
            "provider_type": "azure_cosmos_mongo_db",
            "database_name": "byoebv1",
            "user_collection": "byoebusers",
            "message_collection": "byoebmessages"
        }
    },
    "embeddings": {
        "azure": {
            "model": "text-embedding-3-large",
            "endpoint": "https://swasthyabot-oai-vision.openai.azure.com/",
            "deployment_name": "text-embedding-3-large",
            "api_version": "2023-03-15-preview"
        }
    },
    "llms": {
        "azure": {
            "model": "gpt-4o",
            "endpoint": "https://swasthyabot-oai-vision.openai.azure.com/",
            "deployment_name": "gpt-4o",
            "api_version": "2023-03-15-preview"
        }
    },
    "media_storage": {
        "azure": {
            "account_url": "https://kgretrieval.blob.core.windows.net",
            "container_name": "testcontainerbyoeb"
        }
    },
    "message_queue": {
        "azure": {
            "account_url": "https://kgretrieval.queue.core.windows.net",
            "queue_status": "statusmessages",
            "queue_bot": "botmessages",
            "queue_channel": "channelmessages",
            "messages_per_page": 5,
            "visibility_timeout": 120,
            "time_to_live": 604800
        }
    },
    "translators": {
        "speech":{
            "azure": {
                "resource_id": "/subscriptions/cef13953-6a76-4434-9a65-1d95481f83c7/resourceGroups/swasthyabot/providers/Microsoft.CognitiveServices/accounts/swasthyabot-speech2",
                "region": "centralindia"
            }
        },
        "text": {
            "azure": {
                "resource_id": "/subscriptions/cef13953-6a76-4434-9a65-1d95481f83c7/resourceGroups/swasthyabot/providers/Microsoft.CognitiveServices/accounts/swasthyabot-translator",
                "region": "centralindia"
            }
        }
    },
    "vector_store": {
        "chroma": {
            "collection_name": "vector_collection"
        }
    }
}
