{
    "azure": {
        "cognitive_service_enpoint": "<Cognitive_Service_Endpoint>",
        "speech_translator": {
            "resource_id": "<Speech_Translator_Resource_ID>",
            "region": "<Region>"
        },
        "text_translator": {
            "resource_id": "<Text_Translator_Resource_ID>",
            "region": "<Region>"
        },
        "storage_account": {
            "account_url": "<Storage_Account_URL>",
            "container_name": "<Container_Name>",
            "queue_bot": "<Bot_Queue_Name>",
            "queue_channel": "<Channel_Queue_Name>",
            "messages_per_page": 5,
            "visibility_timeout": 30
        },
        "cosmos_db": {
            "mongo_db_connection_string": "<MongoDB_Connection_String>"
        },
        "embeddings": {
            "model": "text-embedding-3-large",
            "endpoint": "<Embeddings_Endpoint>",
            "deployment_name": "text-embedding-3-large",
            "api_version": "2023-03-15-preview",
            "batch_size": 5
        },
        "llm": {
            "model": "gpt-4o",
            "endpoint": "<LLM_Endpoint>",
            "api_version": "2023-03-15-preview",
            "batch_size": 5
        }
    },
    "vector_store": {
        "chroma": {
            "collection_name": "vector_collection"
        }
    },
    "whatsapp": {
        "allowed_message_types": [
            "audio",
            "text"
        ],
        "batch_size": 5,
        "reuse_client": true
    }
